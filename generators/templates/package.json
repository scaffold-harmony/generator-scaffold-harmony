{
  "name": "<%= projectTitle %>",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "config": {
    "contracts_build_directory": "frontend/src/contracts"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "gulp",
    "localnet:build": "docker build -t harmonyone/localnet-ganache https://github.com/harmony-one/harmony-one-ganache-support.git\\#main:docker",
    "localnet:start": "docker run --name harmony-localnet-ganache --rm -d -p 9500:9500 -p 9800:9800 -p 9801:9801 -p 9501:9501 harmonyone/localnet-ganache && ./scripts/wait-for-localnet.sh && ./scripts/fund-onewallet.js",
    "localnet:stop": "docker rm -f harmony-localnet-ganache",
    "localnet:status": "docker ps --filter name=harmony-localnet-ganache",
    "localnet:logs": "docker logs -f harmony-localnet-ganache",
    "localnet:hmy": "docker exec -it harmony-localnet-ganache hmy",
    "deploy:localnet": "truffle migrate --network=localnet",
    "reset:localnet": "truffle migrate --reset --network=localnet",
    "deploy:testnet": "truffle migrate --network=testnet",
    "reset:testnet": "truffle migrate --reset --network=testnet",
    "deploy:mainnet": "truffle migrate --network=mainnet",
    "reset:mainnet": "truffle migrate --reset --network=mainnet",
    "frontend:localnet": "(cd frontend && npm run start)",
    "frontend:testnet": "(cd frontend && npm run start:testnet)",
    "frontend:mainnet": "(cd frontend && npm run start:mainnet)",
    "postinstall": "(cd frontend && npm install) && npm run localnet:build"
  },
  "keywords": [
    "harmony"
  ],
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^2.4.0"
  },
  "devDependencies": {
    "@trufflesuite/web3-provider-engine": "^15.0.13-1",
    "concurrently": "^6.2.0",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "ethereumjs-wallet": "^1.0.1",
    "ganache-cli": "^6.12.2",
    "gulp": "^4.0.2",
    "gulp-rev": "^9.0.0",
    "gulp-sequence": "^1.0.0",
    "gulp-shell": "^0.8.0",
    "require-dir": "^1.2.0",
    "solc": "^0.7.6",
    "truffle": "^5.4.0"
  }
}
