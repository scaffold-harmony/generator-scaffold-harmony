{
  "name": "generator-scaffold-harmony",
  "version": "1.0.0",
  "description": "yeoman generator for Harmony smart contract projects",
  "homepage": "https://github.com/scaffold-harmony/generator-scaffold-harmony",
  "author": {
    "name": "Peter Chung",
    "email": "touhonoob@gmail.com",
    "url": "https://github.com/touhonoob"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "harmony",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "scaffold-harmony/generator-scaffold-harmony",
  "license": "MIT"
}
